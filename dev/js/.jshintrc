{
    "bitwise": true,
    "browser": true,
    "devel": true,
    "curly": true,
    "eqeqeq": true,
    "es3": true, /* Assume site must work in IE6/7/8. */
    "forin": true,
    "freeze": true,
    "globals": { /* Require and Jasmine's Globals. Note that $ is not permitted. */
        "jQuery": false,
        "define": false,
        "require": false,
        "describe": false, /* Jasmine */
        "xdescribe": false,
        "it": false,
        "xit": false,
        "beforeEach": false,
        "afterEach": false,
        "jasmine": false,
        "spyOn": false,
        "expect": false,
        "waitsFor": false
    },
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonew": true,
    "onevar": true,
    "plusplus": false,
    "quotmark": "single", /* Use backslashes if they\'re needed. */
    "regexp": false,
    "regexdash": true,
    "strict": true, /* Use one 'use strict'; per file. */
    "trailing": true, /* Turn 'show whitespace' on in your editor. */
    "undef": true,
    "unused": true,
    "white": true
}
